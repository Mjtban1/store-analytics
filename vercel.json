{
  "version": 2,
  "public": true,
  "framework": "create-react-app",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "build",
  "installCommand": "npm install --prefer-offline --no-audit --no-optional",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "github": {
    "silent": true,
    "autoAlias": true
  },
  "env": {
    "CI": "false",
    "NODE_ENV": "production",
    "NPM_RC": "legacy-peer-deps=true\nstrict-ssl=false"
  }
}
