{
  "version": 2,
  "public": true,
  "framework": null,
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps --no-audit --ignore-scripts",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate"
        }
      ]
    }
  ],
  "env": {
    "NPM_FLAGS": "--legacy-peer-deps --no-audit"
  }
}
