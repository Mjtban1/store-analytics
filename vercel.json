{
  "version": 2,
  "public": true,
  "installCommand": "npm install",
  "buildCommand": "node node_modules/react-scripts/bin/react-scripts.js build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "NODE_OPTIONS": "--max_old_space_size=4096"
  }
}
