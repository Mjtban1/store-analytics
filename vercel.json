{
  "version": 2,
  "public": true,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --force --legacy-peer-deps --no-audit --unsafe-perm",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "NODE_OPTIONS": "--max_old_space_size=4096",
    "PATH": "/vercel/path0/node_modules/.bin:$PATH",
    "NPM_FLAGS": "--force --legacy-peer-deps --no-audit --unsafe-perm"
  }
}
