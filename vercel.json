{
  "version": 2,
  "public": true,
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --force --legacy-peer-deps --production=false",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CI": "false",
    "NPM_CONFIG_PRODUCTION": "false",
    "NODE_OPTIONS": "--max_old_space_size=4096"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  }
}
